<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO" monitorInterval="60" shutdownHook="disable">
       <Appenders>
         <Console name="Console" target="SYSTEM_OUT">
           <PatternLayout pattern="[%d][%-5p][%t] %c [%5L] - %X %m%n"/>
         </Console>
       </Appenders>
       <Loggers>
         <Logger name="io.netty" level="debug"/>
         <Logger name="reactor.netty.resources.PooledConnectionProvider" level="debug"/>
         <Logger name="com.sai" level="info"/>
         <Root level="info">
           <AppenderRef ref="Console"/>
         </Root>
       </Loggers>

</Configuration>
